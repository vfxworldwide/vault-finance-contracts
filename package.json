    "build": "npx hardhat compile",
    "size": "npx hardhat size-contracts",
    "clean": "rm -rf artifacts && rm -rf cache && rm -rf types && rm -rf flatten/*",
    "balance": "npx hardhat run --network mainnet scripts/account-balance.ts",
    "deploy-factory": "npx hardhat run --network mainnet scripts/deploy-factory.ts",
    "deploy-test-factory": "npx hardhat run --network testnet scripts/deploy-factory.ts",
    "deploy-router": "npx hardhat run --network mainnet scripts/deploy-router.ts",
    "deploy-test-router": "npx hardhat run --network testnet scripts/deploy-router.ts",
    "deploy-swap": "npx hardhat run --network mainnet scripts/deploy-swap.ts",
    "deploy-test-swap": "npx hardhat run --network testnet scripts/deploy-swap.ts",
    "deploy-fee-factory": "npx hardhat run --network mainnet scripts/deploy-fee-factory.ts",
    "deploy-test-fee-factory": "npx hardhat run --network testnet scripts/deploy-fee-factory.ts",
    "deploy-dist": "npx hardhat run --network mainnet scripts/deploy-fee-dist.ts",
    "deploy-test-dist": "npx hardhat run --network testnet scripts/deploy-fee-dist.ts",
    "deploy-moc": "npx hardhat run --network mainnet scripts/deploy-moc.ts",
    "deploy-test-moc": "npx hardhat run --network testnet scripts/deploy-moc.ts",
    "deploy-vault": "npx hardhat run --network mainnet scripts/deploy-vault.ts",
    "deploy-test-vault": "npx hardhat run --network testnet scripts/deploy-vault.ts",
    "deploy-pcs-agent": "npx hardhat run --network mainnet scripts/deploy-pcs-agent.ts",
    "deploy-test-pcs-agent": "npx hardhat run --network testnet scripts/deploy-pcs-agent.ts",
    "testnet-test": "npx hardhat run --network testnet scripts/test-swap.ts",
